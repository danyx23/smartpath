{{#site.currentPage.summary}}
<div class="markdown">{{{site.currentPage.summary}}}</div>
{{/site.currentPage.summary}}

{{#site.currentPage.remarks}}
<h4>{{strings.remarks}}</h4>
<div class="markdown">{{{site.currentPage.remarks}}}</div>
{{/site.currentPage.remarks}}

<div class="markdown">
<pre class="syntax-pre"><code id="syntax" class="language-csharp"></code></pre>
<div id="dummy-syntax" class="hide">{{site.currentPage.linkedSyntax}}</div>
</div>
{{#site.currentPage.classDiagram}}
  <div id="sd-class-diagram" class="svgpan" style="height:{{site.currentPage.classDiagramHeight}}px;" data-fit="{{site.currentPage.fit}}">
    {{{site.currentPage.classDiagram}}}
  </div>
  <div class="legend">
<p class="save"><a href="#"><i class="icon-save"></i>{{strings.save}}</a></p>
<p class="resetZoom"><i class="icon-refresh"></i>{{strings.reset}}</p>
<p>{{strings.panZoom}}</p>
</div>
{{/site.currentPage.classDiagram}}        

{{#site.currentPage.example}}
<h4>{{strings.example}}</h4>
<div class="markdown">{{{site.currentPage.example}}}</div>
{{/site.currentPage.example}}

{{#site.currentPage.returns}}
